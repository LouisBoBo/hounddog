<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="@dimen/dp_2"
        android:layout_height="@dimen/dp_25"
        android:background="@color/color_282828"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_10"/>

    <RelativeLayout
        android:id="@+id/rl_compass"
        android:layout_width="@dimen/dp_245"
        android:layout_height="@dimen/dp_245"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_40"
        android:layout_marginBottom="@dimen/dp_30">

        <RelativeLayout
            android:id="@+id/rl_compass_parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/icon_compass">

            <RelativeLayout
                android:id="@+id/rl_triangle"
                android:layout_width="@dimen/dp_165"
                android:layout_height="@dimen/dp_165"
                android:layout_centerInParent="true">

            </RelativeLayout>
        </RelativeLayout>

        <View
            android:layout_width="@dimen/dp_18"
            android:layout_height="@dimen/dp_18"
            android:layout_centerInParent="true"
            android:background="@drawable/bg_b9b9b8_circle_9"/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_10"
        android:layout_below="@+id/rl_compass"
        android:background="@color/color_E5E5E5"/>

</RelativeLayout>